[mysqld]
character-set-server=utf8
skip-character-set-client-handshake
default-storage-engine=INNODB
host_cache_size=0
skip-name-resolve
general_log=1
general_log_file=/var/log/mysql/general.logbox

[mysqldump]
default-character-set=utf8

[mysql]
default-character-set=utf8

[client]
default-character-set=utf8